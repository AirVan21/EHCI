        call detectStateChangeControl2
        call stopAQueueinCMD

        ; x2 (IN)
        mov al, [EDInEndpointAddress]
        and ax, 0Fh 
        mov [ValidEndpoint], al 
        mov [QHEndpointParam], 02006000h        
        
        mov [InBytes], 0Dh 
        call createQueueHead
        call createBulkInQTD
        call runAQueueinCMD
        call detectStateChangeControl
        call stopAQueueinCMD
        call printQTDStatus

        mov eax, [BulkInQTDToken]
        xor eax, 80000000h
        mov [BulkInQTDToken], eax 
     

        ;------------------------;
        pop ecx                  ; 
        pop ebx                  ; 
        pop eax                  ; 

        ret

    endp

    ; Saves 4KB transaction 
    ; 
    saveReadDataInFile proc near

        push eax                 ;
        push edx                 ;
        push bx                  ;
        push cx                  ;
        ;------------------------;
        
        xor edx, edx 

        ; Create File 
        xor cx, cx 
        lea dx, sDumpFile
        mov ah, 3Ch 
        int 21h

        ; Write to File  
        mov bx, ax 
        
        ; Points Address
        xor eax, eax 
        mov ax, ds 
        shl eax, 4 
        mov edx, [LinearWriteBufferAddress]
        sub edx, eax  
        mov ah, 40h 
        mov cx, 1000h 
        int 21h

        ;Close File 
        mov ah,3Eh              
        int 21h

        ;------------------------;
        pop cx                   ;
        pop bx                   ;
        pop edx                  ; 
        pop eax                  ; 

        ret

    endp

endm
